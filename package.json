  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "server:dev": "cross-env NODE_ENV=development tsx watch server/index.ts --port 3000",
    "client:dev": "vite dev --port 5000 --host 0.0.0.0",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js --port 3000",
    "start:dev": "npm run dev",
    "preview": "npm run build && npm start",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "clean": "rm -rf dist node_modules/.vite",
    "reset": "npm run clean && npm install"
