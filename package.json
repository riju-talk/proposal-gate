{
  "name": "iiitd-sc-proposal-gate",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "server:dev": "cross-env NODE_ENV=development INTEGRATED_DEV=false tsx watch server/index.ts --ignore-watch 'client/**' --ignore-watch 'dist/**' --ignore-watch '**/*.timestamp-*.mjs' --clear-screen=false",
    "client:dev": "vite dev --port 5000 --host 0.0.0.0 --clearScreen false",
    "dev": "concurrently --kill-others-on-fail --prefix-colors \"blue,green\" --names \"ðŸš€ SERVER,âš¡ CLIENT\" \"npm run server:dev\" \"npm run client:dev\"",
    "dev:integrated": "cross-env NODE_ENV=development INTEGRATED_DEV=true tsx watch server/index.ts --ignore-watch 'dist/**' --ignore-watch '**/*.timestamp-*.mjs' --clear-screen=false",
    "build": "npm run clean:dist && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir ../dist/public",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify --target=node20",
    "clean:dist": "rimraf dist",
    "postbuild": "npm run type-check",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "preview": "npm run build && npm start",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "clean": "rimraf dist node_modules/.vite",
    "reset": "npm run clean && npm install",
    "check": "npm run type-check"
  },
